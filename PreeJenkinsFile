pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    
        stage('Build') {
            steps {
                echo 'Hello World building'
            }
        }
            
        stage('Deploy') {
            steps {
                echo 'Hello World Deploying'
            }
        }
            
        stage('Test') {
            steps {
                echo 'Hello World Testing'
            }
        }
            
        stage('Release') {
            steps {
                echo 'Hello World Releasing'
            }
        }
    }
}
